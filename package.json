{
  "name": "@faasjs/provider-tencentcloud-scf",
  "version": "0.0.0-alpha.1",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -p tsconfig.json --fix",
    "build": "rm -rf ./lib && tsc",
    "test": "jest --expand",
    "doc": "jsdoc2md --files ./lib/*.js > ./API.md",
    "ci": "yarn lint && yarn test",
    "commit": "yarn ci && yarn build && yarn doc"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@faasjs/utils": "0.0.0-alpha.2",
    "cos-nodejs-sdk-v5": "*"
  },
  "devDependencies": {
    "@babel/core": "*",
    "@babel/preset-env": "*",
    "@babel/preset-typescript": "*",
    "@types/debug": "*",
    "@types/jest": "*",
    "@types/node": "*",
    "babel-jest": "*",
    "jest": "*",
    "jsdoc-babel": "*",
    "jsdoc-to-markdown": "*",
    "tslint": "*",
    "typescript": "*"
  },
  "jest": {
    "collectCoverage": true,
    "testRegex": "/*\\.test\\.ts$",
    "testPathIgnorePatterns": [
      "/lib/",
      "/tmp/"
    ],
    "coveragePathIgnorePatterns": [
      "/lib/",
      "/tmp/",
      "/__tests__/"
    ],
    "modulePathIgnorePatterns": [
      "/lib/",
      "/tmp/"
    ],
    "watchPathIgnorePatterns": [
      "/lib/",
      "/tmp/"
    ]
  }
}